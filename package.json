{
  "name": "http-eval",
  "version": "0.0.0",
  "type": "module",
  "description": "Eval HTTP POST bodies in NodeJS",
  "author": "Ben Creech",
  "license": "MIT",
  "homepage": "https://github.com/bpcreech/http-eval",
  "module": "./dist/index.mjs",
  "exports": {
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "pkgroll",
    "start": "node dist/index.mjs",
    "dev": "tsx --watch src/cli.ts",
    "lint": "eslint src/**/*.ts tests/**/*.ts",
    "format": "eslint src/**/*.ts tests/**/*.ts --fix",
    "//": "We don't use jest because of https://github.com/jestjs/jest/issues/9430",
    "test": "tsx --test tests/**/*test.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bpcreech/http-eval.git"
  },
  "bugs": {
    "url": "https://github.com/bpcreech/http-eval/issues"
  },
  "keywords": [
    "http",
    "eval",
    "RCE"
  ],
  "author": "Ben Creech",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "pkgroll": "^2.0.1",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/express-serve-static-core": "^4.17.43",
    "@types/express": "^4.17.21",
    "express": "^4.18.2"
  }
}
