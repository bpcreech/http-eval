    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.2.2",
    "vite": "^5.1.0",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-dts": "^3.7.2",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^1.2.2"
